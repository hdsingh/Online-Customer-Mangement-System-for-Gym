<% include partials/admin_header %>

<style type="text/css">
  body{
    background: linear-gradient(to right, #12c2e9, #c471ed, #f64f59);
  }
</style>

<div class="container">
	<div class="text-center">
		<h1><i class="fa fa-user fa-2x" aria-hidden="true"></i></h1>
	</div>
<div class="form-control mb-5 mt-5">
<div class="table-wrapper">
   <table class="table table-striped table-hover ">
      <tr class="bg-warning text-center"><th><h3>New Members Sign up</h3></th></tr>
   </table>
   </div>
<div class="table-wrapper">
   <table class="table table-hover table-striped ">
      	<thead>
      		<tr>
      			<th>#</th>
      			<th>Name</th>
      			<th>Mobile</th>
      		</tr>
      	</thead>
      	<tbody>
<% var count = 1%>
<%members.forEach(function(member){ %>
	<tr>
		<td><h4><%=count%></h4></td>
     	<td><h4><%=member.name.toUpperCase()%></h4></td>
	    <td><h4><%=member.mobile%><td><h4>
    </tr>
    <%count+=1%>
<%})%>
</tbody>
</table>
</div>
</div>
